{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "unitone",
	"name": "unitone/child-pages",
	"title": "Child pages",
	"description": "This block displays child pages.",
	"attributes": {
		"showTopLevel": {
			"type": "boolean",
			"default": false
		},
		"parent": {
			"type": "object",
			"default": {}
		},
		"columnMinWidth": {
			"type": "string"
		}
	},
	"supports": {
		"className": false,
		"anchor": true,
		"unitone": {
			"gap": {
				"styles": {
					"cluster-divided-stripe": true,
					"cluster-divided-slash": true,
					"stack-divided-stripe": true,
					"rich-media": true,
					"box": true,
					"panel": true
				}
			}
		}
	},
	"styles": [
		{
			"name": "default",
			"label": "Default",
			"isDefault": true
		},
		{
			"name": "cluster-divided-stripe",
			"label": "Cluster (Stripe)"
		},
		{
			"name": "cluster-divided-slash",
			"label": "Cluster (Slash)"
		},
		{
			"name": "stack-divided-stripe",
			"label": "Stack (Stripe)"
		},
		{
			"name": "rich-media",
			"label": "Rich media"
		},
		{
			"name": "box",
			"label": "Box"
		},
		{
			"name": "panel",
			"label": "Panel"
		}
	],
	"category": "unitone/component",
	"style": "file:./style-index.css",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}
