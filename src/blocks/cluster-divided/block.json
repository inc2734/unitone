	"apiVersion": 2,
	"textdomain": "unitone",
	"name": "unitone/cluster-divided",
	"title": "Cluster (Divided)",
	"description": "This block arranges children horizontally.",
	"attributes": {
		"tagName": {
			"type": "string"
		}
	},
	"providesContext": {
		"unitone/cluster-divided/tagName": "tagName"
	},
	"supports": {
