{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "unitone",
	"name": "unitone/cover",
	"title": "Cover",
	"description": "This block is designed to cover a large area of the screen.",
	"attributes": {
		"noPadding": {
			"type": "boolean",
			"default": false
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"supports": {
		"className": false,
		"anchor": true,
		"unitone": {
			"gap": true,
			"minHeight": true
		}
	},
	"category": "unitone/layout",
	"editorScript": "file:./index.js"
}
