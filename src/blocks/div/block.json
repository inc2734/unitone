{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "21.0.0",
	"textdomain": "unitone",
	"name": "unitone/div",
	"title": "div",
	"description": "A simple div element without any decoration.",
	"attributes": {
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"selectors": {
		"root": ".unitone-div"
	},
	"supports": {
		"className": false,
		"anchor": true,
		"align": [ "full", "wide" ],
		"allowedBlocks": true,
		"unitone": {
			"style": true
		}
	},
	"category": "unitone/component",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
