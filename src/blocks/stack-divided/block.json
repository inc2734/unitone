{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "14.4.2",
	"textdomain": "unitone",
	"name": "unitone/stack-divided",
	"title": "Stack (Divided)",
	"description": "This block is used to arrange children vertically at regular intervals.",
	"attributes": {
		"tagName": {
			"type": "string",
			"default": "div"
		},
		"unitone": {
			"type": "object",
			"default": {
				"dividerType": "stripe",
				"divider": {
					"style": "solid",
					"width": "1px"
				},
				"dividerColor": "unitone-light-gray"
			}
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"providesContext": {
		"unitone/stack-divided/tagName": "tagName"
	},
	"selectors": {
		"root": "[data-unitone-layout~=stack][data-unitone-layout*=-divider:]"
	},
	"supports": {
		"className": false,
		"anchor": true,
		"color": {
			"background": false,
			"link": true,
			"text": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalTextDecoration": true,
			"__experimentalFontStyle": true,
			"__experimentalFontWeight": true,
			"__experimentalLetterSpacing": true,
			"__experimentalTextTransform": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		},
		"unitone": {
			"gap": true,
			"dividerType": [ "stripe", "underline", "bordered" ],
			"divider": true,
			"fluidTypography": true,
			"lineHeight": true
		}
	},
	"example": {
		"innerBlocks": [
			{
				"name": "core/paragraph",
				"attributes": {
					"content": "Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua."
				}
			},
			{
				"name": "core/paragraph",
				"attributes": {
					"content": "Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat."
				}
			},
			{
				"name": "core/paragraph",
				"attributes": {
					"content": "Duis aute irure dolor in reprehenderit in voluptate velit esse cill."
				}
			}
		]
	},
	"category": "unitone/layout",
	"editorScript": "file:./index.js",
	"style": [ "file:./style-index.css", "unitone-stack-style" ],
	"editorStyle": "file:./index.css"
}
