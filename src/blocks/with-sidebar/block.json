{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "unitone",
	"name": "unitone/with-sidebar",
	"title": "With sidebar",
	"description": "This block is used to assemble components with sidebars.",
	"attributes": {
		"sidebarWidth": {
			"type": "string"
		},
		"contentMinWidth": {
			"type": "string"
		},
		"contentMaxWidth": {
			"type": "string"
		},
		"revert": {
			"type": "boolean",
			"default": false
		},
		"sidebar": {
			"type": "string"
		}
	},
	"supports": {
		"className": false,
		"anchor": true,
		"typography": {
			"fontSize": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		},
		"unitone": {
			"gap": true,
			"alignItems": true
		}
	},
	"category": "unitone/layout",
	"editorScript": "file:./index.js"
}
