{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 2,
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"gradients": [
				{
					"slug":"unitone-accent",
					"gradient":"linear-gradient(135deg,hsl(276,49%,47%) 0%,hsl(253,49%,17%) 100%)",
					"name":"Accent gradation"
				}
			],
			"palette": [
				{
					"slug": "unitone-accent",
					"color": "#1f1640",
					"name": "Accent"
				},
				{
					"slug": "unitone-background",
					"color": "var(--wp--preset--color--white)",
					"name": "Background"
				},
				{
					"slug": "unitone-background-alt",
					"color": "var(--wp--preset--color--unitone-light-gray)",
					"name": "Background Alt"
				},
				{
					"slug": "unitone-text",
					"color": "var(--unitone--color--text)",
					"name": "Text"
				},
				{
					"slug": "unitone-text-alt",
					"color": "var(--wp--preset--color--white)",
					"name": "Text Alt"
				},
				{
					"slug": "unitone-text-black",
					"color": "var(--unitone--color--text)",
					"name": "Text (Black)"
				},
				{
					"slug": "unitone-bright-gray",
					"color": "rgba(4, 27, 62, .025)",
					"name": "Bright gray"
				},
				{
					"slug": "unitone-light-gray",
					"color": "rgba(4, 27, 62, .07)",
					"name": "Light gray"
				},
				{
					"slug": "unitone-gray",
					"color": "rgba(4, 27, 62, .19)",
					"name": "gray"
				},
				{
					"slug": "unitone-dark-gray",
					"color": "rgba(4, 27, 62, .42)",
					"name": "Dark gray"
				}
			]
		},
		"custom": {
			"container-max-width": "min(100%, var(--unitone--container-max-width))",
			"content-max-width": "min(100%, var(--unitone--measure))"
		},
		"layout": {
			"contentSize": "var(--wp--custom--content-max-width)",
			"wideSize": "var(--wp--custom--container-max-width)"
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"textTransform": true,
			"lineHeight": true,
			"fontSizes": [
				{
					"slug": "unitone-xs",
					"size": "0.8rem",
					"name": "XS"
				},
				{
					"slug": "unitone-s",
					"size": "0.88rem",
					"name": "S"
				},
				{
					"slug": "unitone-m",
					"size": "1rem",
					"name": "M"
				},
				{
					"slug": "unitone-l",
					"size": "1.14rem",
					"name": "L"
				},
				{
					"slug": "unitone-xl",
					"size": "1.33rem",
					"name": "XL"
				},
				{
					"slug": "unitone-2xl",
					"size": "1.6rem",
					"name": "2XL"
				},
				{
					"slug": "unitone-3xl",
					"size": "2rem",
					"name": "3XL"
				},
				{
					"slug": "unitone-4xl",
					"size": "2.66rem",
					"name": "4XL"
				},
				{
					"slug": "unitone-5xl",
					"size": "4rem",
					"name": "5XL"
				},
				{
					"slug": "unitone-6xl",
					"size": "8rem",
					"name": "6XL"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"padding": false,
			"spacingSizes": [
				{
					"name": "-2",
					"slug": "20",
					"size": "var(--unitone--s-2)"
				},
				{
					"name": "-1",
					"slug": "30",
					"size": "var(--unitone--s-1)"
				},
				{
					"name": "1",
					"slug": "40",
					"size": "var(--unitone--s1)"
				},
				{
					"name": "2",
					"slug": "50",
					"size": "var(--unitone--s2)"
				},
				{
					"name": "3",
					"slug": "60",
					"size": "var(--unitone--s3)"
				},
				{
					"name": "4",
					"slug": "70",
					"size": "var(--unitone--s4)"
				},
				{
					"name": "5",
					"slug": "80",
					"size": "var(--unitone--s5)"
				}
			]
		},
		"blocks": {
			"core/post-template": {
				"layout": {
					"allowEditing": false
				},
				"spacing": {
					"blockGap": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--unitone-background)",
			"text": "var(--wp--preset--color--unitone-text)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--unitone-accent)"
				},
				"border": {
					"radius": "var(--unitone--global--border-radius)"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--unitone--padding) / 1.5)",
						"right": "var(--unitone--padding)",
						"bottom": "calc(var(--unitone--padding) / 1.5)",
						"left": "var(--unitone--padding)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--unitone-accent)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--unitone-dark-gray)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--unitone-s)"
				},
				"spacing": {
					"margin": {
						"top": "var(--unitone--s-2)",
						"right": "0",
						"bottom": "0",
						"left": "0"
					}
				}
			}
		},
		"blocks": {
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--unitone-text)",
					"text": "var(--wp--preset--color--unitone-text-alt)"
				}
			},
			"core/social-links": {
				"typography": {
					"fontSize": "inherit"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "inherit",
					"lineHeight": "inherit"
				}
			},
			"core/table": {
				"border": {
					"width": "0px",
					"style": "solid",
					"color": "var(--wp--preset--color--unitone-gray)"
				},
				"variations": {
					"stripes": {
					},
					"underline": {
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--unitone--global--text-gap)"
		}
	},
