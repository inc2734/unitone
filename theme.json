	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"gradients": [
				{
					"slug":"unitone-accent",
					"gradient":"linear-gradient(135deg,hsl(276,49%,47%) 0%,hsl(253,49%,17%) 100%)",
					"name":"Accent gradation"
				}
			],
			"palette": [
				{
					"slug": "unitone-accent",
					"color": "#1f1640",
					"name": "Accent"
				},
				{
					"slug": "unitone-background",
