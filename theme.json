					"fontSize": "inherit"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "inherit",
					"lineHeight": "inherit"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--unitone--global--gap)"
		}
