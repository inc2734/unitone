					"name": "Text"
				},
				{
					"slug": "unitone-text-alt",
					"color": "var(--wp--preset--color--white)",
					"name": "Text Alt"
				},
				{
					"slug": "unitone-text-black",
					"color": "var(--unitone--color--text)",
					"name": "Text (Black)"
				},
				{
					"slug": "unitone-bright-gray",
					"color": "rgba(4, 27, 62, .025)",
					"name": "Bright gray"
				},
				{
					"slug": "unitone-light-gray",
					"color": "rgba(4, 27, 62, .07)",
					"name": "Light gray"
				},
				{
					"slug": "unitone-gray",
					"color": "rgba(4, 27, 62, .19)",
					"name": "gray"
				},
				{
					"slug": "unitone-dark-gray",
					"color": "rgba(4, 27, 62, .42)",
					"name": "Dark gray"
				}
			]
		},
