{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 2,
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"gradients": [
				{
					"slug":"unitone-accent",
					"gradient":"linear-gradient(135deg,hsl(276,49%,47%) 0%,hsl(253,49%,17%) 100%)",
					"name":"Accent color gradation"
				}
			],
			"palette": [
				{
					"slug": "unitone/accent",
					"color": "#1f1640",
					"name": "Accent color"
				},
				{
					"slug": "unitone/text",
					"color": "var(--unitone--color--text)",
					"name": "text"
				},
				{
					"slug": "unitone/bright-gray",
					"color": "var(--unitone--color--bright-gray)",
					"name": "Bright gray"
				},
				{
					"slug": "unitone/light-gray",
					"color": "var(--unitone--color--light-gray)",
					"name": "Light gray"
				},
				{
					"slug": "unitone/gray",
					"color": "var(--unitone--color--gray)",
					"name": "gray"
				},
				{
					"slug": "unitone/dark-gray",
					"color": "var(--unitone--color--dark-gray)",
					"name": "Dark gray"
				}
			]
		},
		"custom": {
			"container-max-width": "min(100% - var(--unitone--rem1) * 2, var(--unitone--container-max-width))",
			"content-max-width": "min(100% - var(--unitone--rem1) * 2, var(--unitone--measure))"
		},
		"layout": {
			"contentSize": "var(--wp--custom--content-max-width)",
			"wideSize": "var(--wp--custom--container-max-width)"
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"textTransform": true,
			"lineHeight": true,
			"fontSizes": [
				{
					"slug": "unitone-xs",
					"size": "0.8rem",
					"name": "XS"
				},
				{
					"slug": "unitone-s",
					"size": "0.88rem",
					"name": "S"
				},
				{
					"slug": "unitone-m",
					"size": "1rem",
					"name": "M"
				},
				{
					"slug": "unitone-l",
					"size": "1.14rem",
					"name": "L"
				},
				{
					"slug": "unitone-xl",
					"size": "1.33rem",
					"name": "XL"
				},
				{
					"slug": "unitone-2xl",
					"size": "1.6rem",
					"name": "2XL"
				},
				{
					"slug": "unitone-3xl",
					"size": "2rem",
					"name": "3XL"
				},
				{
					"slug": "unitone-4xl",
					"size": "2.66rem",
					"name": "4XL"
				},
				{
					"slug": "unitone-5xl",
					"size": "4rem",
					"name": "5XL"
				},
				{
					"slug": "unitone-6xl",
					"size": "8rem",
					"name": "6XL"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"padding": false
		}
	},
	"styles": {
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--unitone-accent)"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|unitone-accent"
				}
			}
		},
		"blocks": {
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--unitone--color--white)"
				}
			},
			"core/search": {
				"color": {
					"background": "var(--wp--preset--color--black)"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						"color": {
							"background": "var(--wp--preset--color--unitone-accent)",
							"text": "var(--wp--preset--color--white)"
						}
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--unitone--s1)"
		}
	},
